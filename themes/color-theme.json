{
  "$schema": "vscode://schemas/color-theme",
  "name": "Dark Purple Candy",
  "type": "dark",
  "semanticHighlighting": true,
  "colors": {
    "focusBorder": "#FF77AA66",
    "foreground": "#E0E0E0",
    "errorForeground": "#FF7A85",
    "editor.background": "#1E0E2F",
    "editor.foreground": "#EDE1F6",
    "editorCursor.foreground": "#FFCCFF",
    "editorLineNumber.foreground": "#6B5A77",
    "editorLineNumber.activeForeground": "#FF88CC",
    "editor.selectionBackground": "#3A1F4D",
    "editor.selectionHighlightBackground": "#4D316066",
    "editor.wordHighlightBackground": "#4DD9D926",
    "editor.wordHighlightStrongBackground": "#FF77AA29",
    "editor.lineHighlightBackground": "#2A1740",
    "editorWhitespace.foreground": "#3B274B",
    "editorIndentGuide.background": "#332141",
    "editorIndentGuide.activeBackground": "#52315E",
    "editor.hoverHighlightBackground": "#4D31604D",
    "editor.findMatchBackground": "#FF77AA55",
    "editor.findMatchHighlightBackground": "#FFB84D33",
    "editorBracketMatch.background": "#4DD9D926",
    "editorBracketMatch.border": "#4DD9D98C",

    "activityBar.background": "#1A0A28",
    "activityBar.foreground": "#D8C2F0",
    "activityBarBadge.background": "#FF77AA",
    "activityBarBadge.foreground": "#FFFFFF",
    "activityBar.inactiveForeground": "#8B6DA6",
    "activityBar.border": "#2F1A43",

    "sideBar.background": "#241032",
    "sideBar.foreground": "#B9A7D9",
    "sideBar.border": "#2F1A43",
    "sideBarSectionHeader.background": "#2A123B",
    "sideBarSectionHeader.foreground": "#E6CCE6",
    "sideBarTitle.foreground": "#D0C0E0",

    "statusBar.background": "#3B1C50",
    "statusBar.foreground": "#E6CCE6",
    "statusBar.debuggingBackground": "#FF4D73",
    "statusBar.debuggingForeground": "#1A0A28",
    "statusBar.noFolderBackground": "#2A123B",
    "statusBar.noFolderForeground": "#E6CCE6",
    "statusBarItem.hoverBackground": "#FF77AA",
    "statusBarItem.remoteBackground": "#FF99DD",
    "statusBarItem.remoteForeground": "#1A0A28",

    "titleBar.activeBackground": "#2A123B",
    "titleBar.activeForeground": "#D0C0E0",
    "titleBar.inactiveBackground": "#1E0E2F",
    "titleBar.inactiveForeground": "#A080B0",
    "menubar.selectionBackground": "#3A1F4D",
    "menubar.selectionForeground": "#F2E6FF",

    "editorGroup.border": "#2F1A43",
    "editorGroup.dropBackground": "#FF77AA33",
    "editorGroupHeader.tabsBackground": "#1E0E2F",
    "tab.activeBackground": "#2A123B",
    "tab.activeForeground": "#FFFFFF",
    "tab.border": "#2F1A43",
    "tab.inactiveBackground": "#1E0E2F",
    "tab.inactiveForeground": "#B9A7D9",
    "tab.hoverBackground": "#331743",
    "tab.unfocusedActiveForeground": "#D0C0E0",

    "panel.background": "#1C0E2C",
    "panel.border": "#2F1A43",
    "panelTitle.activeForeground": "#FFCFED",
    "panelTitle.inactiveForeground": "#A080B0",
    "panelTitle.activeBorder": "#FF88CC",

    "peekViewEditor.background": "#2A1740",
    "peekViewEditor.matchHighlightBackground": "#FFB84D44",
    "peekViewResult.background": "#1F0F31",
    "peekViewResult.selectionBackground": "#3A1F4D",
    "peekViewTitle.background": "#1A0A28",

    "list.focusBackground": "#3A1F4D",
    "list.focusForeground": "#FFFFFF",
    "list.activeSelectionBackground": "#4D2A63",
    "list.activeSelectionForeground": "#FFFFFF",
    "list.inactiveSelectionBackground": "#342047",
    "list.hoverBackground": "#2D153F",
    "list.highlightForeground": "#FF77AA",
    "list.errorForeground": "#FF7A85",
    "list.warningForeground": "#FFB84D",

    "gitDecoration.addedResourceForeground": "#99FF99",
    "gitDecoration.modifiedResourceForeground": "#66D9FF",
    "gitDecoration.deletedResourceForeground": "#FF4D73",
    "gitDecoration.untrackedResourceForeground": "#C1FFDC",
    "gitDecoration.ignoredResourceForeground": "#5F4C71",
    "gitDecoration.conflictingResourceForeground": "#FFB84D",

    "button.background": "#FF77AA",
    "button.foreground": "#1A0A28",
    "button.hoverBackground": "#FF8CC2",
    "badge.background": "#FF77AA",
    "badge.foreground": "#1A0A28",

    "input.background": "#271238",
    "input.border": "#3B1C50",
    "input.foreground": "#F2E6FF",
    "input.placeholderForeground": "#9785AA",
    "inputValidation.errorBorder": "#FF7A85",
    "inputValidation.warningBorder": "#FFB84D",
    "inputValidation.infoBorder": "#66D9FF",

    "dropdown.background": "#271238",
    "dropdown.foreground": "#EDE1F6",
    "dropdown.border": "#3B1C50",

    "scrollbarSlider.background": "#6B5A7788",
    "scrollbarSlider.hoverBackground": "#8B76A8AA",
    "scrollbarSlider.activeBackground": "#FF77AAAA",

    "notificationCenterHeader.background": "#2A123B",
    "notifications.background": "#241032",
    "notifications.foreground": "#E6CCE6",
    "notificationsErrorIcon.foreground": "#FF7A85",
    "notificationsWarningIcon.foreground": "#FFB84D",
    "notificationsInfoIcon.foreground": "#66D9FF",

    "settings.headerForeground": "#FFCFED",
    "settings.modifiedItemIndicator": "#FF77AA",

    "terminal.background": "#1E0E2F",
    "terminal.foreground": "#E0E0E0",
    "terminal.selectionBackground": "#3A1F4D",
    "terminalCursor.foreground": "#FFCCFF",
    "terminal.ansiBlack": "#241032",
    "terminal.ansiRed": "#FF4D73",
    "terminal.ansiGreen": "#99FF99",
    "terminal.ansiYellow": "#FFB84D",
    "terminal.ansiBlue": "#66D9FF",
    "terminal.ansiMagenta": "#FF77AA",
    "terminal.ansiCyan": "#4DD9D9",
    "terminal.ansiWhite": "#E0E0E0",
    "terminal.ansiBrightBlack": "#48395B",
    "terminal.ansiBrightRed": "#FF7A85",
    "terminal.ansiBrightGreen": "#B3FFB3",
    "terminal.ansiBrightYellow": "#FFD480",
    "terminal.ansiBrightBlue": "#80E5FF",
    "terminal.ansiBrightMagenta": "#FF99DD",
    "terminal.ansiBrightCyan": "#6FE5E5",
    "terminal.ansiBrightWhite": "#FFFFFF"
  },
  "tokenColors": [
    {
      "scope": "comment",
      "settings": { "foreground": "#6A6A75" }
    },
    {
      "scope": "keyword",
      "settings": { "foreground": "#FF66CC" }
    },
    {
      "scope": "storage.type",
      "settings": { "foreground": "#FF66CC" }
    },
    {
      "scope": "keyword.control",
      "settings": { "foreground": "#FF66CC", "fontStyle": "bold" }
    },
    {
      "scope": "storage.modifier",
      "settings": { "foreground": "#FF88CC" }
    },
    {
      "scope": "operator",
      "settings": { "foreground": "#FFD5FF" }
    },
    {
      "scope": "entity.name.function",
      "settings": { "foreground": "#FF8C69" }
    },
    {
      "scope": "entity.name.class",
      "settings": { "foreground": "#FF99DD" }
    },
    {
      "scope": "entity.name.type",
      "settings": { "foreground": "#66D9FF" }
    },
    {
      "scope": "entity.name.tag",
      "settings": { "foreground": "#FF77AA" }
    },
    {
      "scope": "variable",
      "settings": { "foreground": "#E0E0E0" }
    },
    {
      "scope": ["variable.language", "variable.other.constant"],
      "settings": { "foreground": "#FFB84D" }
    },
    {
      "scope": "variable.parameter",
      "settings": { "foreground": "#C1B9FF" }
    },
    {
      "scope": "variable.other.property",
      "settings": { "foreground": "#FFCFED" }
    },
    {
      "scope": "string",
      "settings": { "foreground": "#4DD9D9" }
    },
    {
      "scope": "string.regexp",
      "settings": { "foreground": "#80E5FF" }
    },
    {
      "scope": "constant.numeric",
      "settings": { "foreground": "#FF7A85" }
    },
    {
      "scope": "constant.language",
      "settings": { "foreground": "#FFB84D" }
    },
    {
      "scope": "support.class",
      "settings": { "foreground": "#FF99DD" }
    },
    {
      "scope": "support.function",
      "settings": { "foreground": "#FF4D73" }
    },
    {
      "scope": "support.constant",
      "settings": { "foreground": "#FFB84D" }
    },
    {
      "scope": "punctuation.definition.tag",
      "settings": { "foreground": "#FF66CC" }
    },
    {
      "scope": "punctuation.definition.string",
      "settings": { "foreground": "#4DD9D9" }
    },
    {
      "scope": "punctuation.section.embedded",
      "settings": { "foreground": "#FF77AA" }
    },
    {
      "scope": "meta.directive",
      "settings": { "foreground": "#66D9FF" }
    },
    {
      "scope": "meta.import",
      "settings": { "foreground": "#FF66CC" }
    },
    {
      "scope": "markup.heading",
      "settings": { "foreground": "#FF99DD", "fontStyle": "bold" }
    },
    {
      "scope": "markup.bold",
      "settings": { "foreground": "#FFCFED", "fontStyle": "bold" }
    },
    {
      "scope": "markup.italic",
      "settings": { "foreground": "#FFCFED", "fontStyle": "italic" }
    },
    {
      "scope": "markup.inline.raw",
      "settings": { "foreground": "#80E5FF" }
    },
    {
      "scope": "markup.quote",
      "settings": { "foreground": "#B9A7D9" }
    },
    {
      "scope": "markup.list",
      "settings": { "foreground": "#FF77AA" }
    },
    {
      "scope": "markup.inserted",
      "settings": { "foreground": "#99FF99" }
    },
    {
      "scope": "markup.deleted",
      "settings": { "foreground": "#FF4D73" }
    },
    {
      "scope": "markup.changed",
      "settings": { "foreground": "#FFB84D" }
    },
    {
      "scope": "meta.diff.header",
      "settings": { "foreground": "#66D9FF" }
    },
    {
      "scope": "constant.character.escape",
      "settings": { "foreground": "#80E5FF" }
    },
    {
      "scope": "invalid",
      "settings": { "foreground": "#FFFFFF", "background": "#FF4D73" }
    },
    {
      "scope": "invalid.deprecated",
      "settings": { "foreground": "#FFFFFF", "background": "#FFB84D" }
    }
  ]
}
