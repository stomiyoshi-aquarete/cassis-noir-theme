{
  "$schema": "vscode://schemas/color-theme",
  "name": "Dark Purple Candy",
  "type": "dark",
  "semanticHighlighting": true,
  "colors": {
    "focusBorder": "#FF77AA66",
    "foreground": "#cac0c9",
    "errorForeground": "#FF7A85",
    "editor.background": "#1B0030",
    "editor.foreground": "#c0acc9",
    "editorCursor.foreground": "#cdb0d0",
    "editorLineNumber.foreground": "#6B5A77",
    "editorLineNumber.activeForeground": "#FF88CC",
    "editor.selectionBackground": "#3C1155",
    "editor.selectionHighlightBackground": "#55206C66",
    "editor.wordHighlightBackground": "#56E0E02C",
    "editor.wordHighlightStrongBackground": "#FF66C433",
    "editor.lineHighlightBackground": "#2B0A45",
    "editorWhitespace.foreground": "#3B0E4F",
    "editorIndentGuide.background": "#32004A",
