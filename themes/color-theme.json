    "activityBarBadge.foreground": "#FFFFFF",
    "activityBar.inactiveForeground": "#662e7b",
    "activityBar.border": "#2A0048",
    "sideBar.background": "#270542",
    "sideBar.foreground": "#b2a5c6",
    "sideBar.border": "#2A0048",
    "sideBarSectionHeader.background": "#240040",
    "sideBarSectionHeader.foreground": "#D4C5E5",
    "sideBarTitle.foreground": "#C5B9D6",
    "statusBar.background": "#320055",
    "statusBar.foreground": "#E4D5EC",
    "statusBar.debuggingBackground": "#FF4D73",
    "statusBar.debuggingForeground": "#1A0A28",
    "statusBar.noFolderBackground": "#240048",
    "statusBar.noFolderForeground": "#E6CCE6",
    "statusBarItem.hoverBackground": "#FF6BC9",
