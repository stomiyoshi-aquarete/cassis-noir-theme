    "editor.hoverHighlightBackground": "#55206C4D",
    "editor.findMatchBackground": "#FF66C460",
    "editor.findMatchHighlightBackground": "#FFAD4040",
    "editorBracketMatch.background": "#56E0E02C",
    "editorBracketMatch.border": "#56E0E0AA",
