  "tokenColors": [
    {
      "scope": "comment",
      "settings": {
        "foreground": "#6A6A75"
      }
    },
    {
      "scope": "keyword",
      "settings": {
        "foreground": "#c458a2"
      }
    },
    {
      "scope": "storage.type",
      "settings": {
        "foreground": "#F066C4"
      }
    },
    {
      "scope": "keyword.control",
      "settings": {
        "foreground": "#F066C4",
        "fontStyle": "bold"
      }
    },
    {
      "scope": "storage.modifier",
      "settings": {
        "foreground": "#F48AD2"
      }
    },
    {
      "scope": "operator",
      "settings": {
        "foreground": "#E6D2F2"
      }
    },
    {
      "scope": "entity.name.function",
      "settings": {
        "foreground": "#F27D6D"
      }
    },
    {
      "scope": "entity.name.class",
      "settings": {
        "foreground": "#F2A4D6"
      }
    },
    {
      "scope": "entity.name.type",
      "settings": {
        "foreground": "#7CC0F0"
      }
    },
    {
      "scope": "entity.name.tag",
      "settings": {
        "foreground": "#F272B8"
      }
    },
    {
      "scope": "variable",
      "settings": {
        "foreground": "#D9D1E6"
      }
    },
    {
      "scope": [
        "variable.language",
        "variable.other.constant"
      ],
      "settings": {
        "foreground": "#c0b858"
      }
    },
    {
      "scope": "variable.parameter",
      "settings": {
        "foreground": "#B3A9EB"
      }
    },
    {
      "scope": "variable.other.property",
      "settings": {
        "foreground": "#E4C9F2"
      }
    },
    {
      "scope": "string",
      "settings": {
        "foreground": "#67D2D2"
      }
    },
    {
      "scope": "string.regexp",
      "settings": {
        "foreground": "#7DD6F0"
      }
    },
    {
      "scope": "constant.numeric",
      "settings": {
        "foreground": "#F38C8E"
      }
    },
    {
      "scope": "constant.language",
      "settings": {
        "foreground": "#e0e269"
      }
    },
    {
      "scope": "support.class",
      "settings": {
        "foreground": "#F2A4D6"
      }
    },
    {
      "scope": "support.function",
      "settings": {
        "foreground": "#F25985"
      }
    },
    {
      "scope": "support.constant",
      "settings": {
        "foreground": "#F0BF74"
      }
    },
    {
      "scope": "punctuation.definition.tag",
      "settings": {
        "foreground": "#F066C4"
      }
    },
    {
      "scope": "punctuation.definition.string",
      "settings": {
        "foreground": "#67D2D2"
      }
    },
    {
      "scope": "punctuation.section.embedded",
      "settings": {
        "foreground": "#F272B8"
      }
    },
    {
      "scope": "meta.directive",
      "settings": {
        "foreground": "#7CC0F0"
      }
    },
    {
      "scope": "meta.import",
      "settings": {
        "foreground": "#F066C4"
      }
    },
    {
      "scope": "markup.heading",
      "settings": {
        "foreground": "#F2A4D6",
        "fontStyle": "bold"
      }
    },
    {
      "scope": "markup.bold",
      "settings": {
        "foreground": "#EDD5F4",
        "fontStyle": "bold"
      }
    },
    {
      "scope": "markup.italic",
      "settings": {
        "foreground": "#EDD5F4",
        "fontStyle": "italic"
      }
    },
    {
      "scope": "markup.inline.raw",
      "settings": {
        "foreground": "#7CC0F0"
      }
    },
    {
      "scope": "markup.quote",
      "settings": {
        "foreground": "#C0B3D9"
      }
    },
    {
      "scope": "markup.list",
      "settings": {
        "foreground": "#F272B8"
      }
    },
    {
      "scope": "markup.inserted",
      "settings": {
        "foreground": "#8CE79A"
      }
    },
    {
      "scope": "markup.deleted",
      "settings": {
        "foreground": "#F25985"
      }
    },
    {
      "scope": "markup.changed",
      "settings": {
        "foreground": "#F0BF74"
      }
    },
    {
      "scope": "meta.diff.header",
      "settings": {
        "foreground": "#7CC0F0"
      }
    },
    {
      "scope": "constant.character.escape",
      "settings": {
        "foreground": "#7DD6F0"
      }
    },
    {
      "scope": "invalid",
      "settings": {
        "foreground": "#FFFFFF",
        "background": "#F25985"
      }
    },
    {
      "scope": "invalid.deprecated",
      "settings": {
        "foreground": "#FFFFFF",
        "background": "#F0BF74"
      }
    }
  ]
}
