    "statusBarItem.remoteForeground": "#231238",
    "titleBar.activeBackground": "#190431",
    "titleBar.activeForeground": "#C8BADB",
    "titleBar.inactiveBackground": "#1A0038",
    "titleBar.inactiveForeground": "#9A87B2",
    "menubar.selectionBackground": "#270439",
    "menubar.selectionForeground": "#E8DEF7",
    "editorGroup.border": "#2A0048",
    "editorGroup.dropBackground": "#FF66C43F",
    "editorGroupHeader.tabsBackground": "#1A0038",
    "tab.activeBackground": "#270448",
    "tab.activeForeground": "#EDE7F5",
    "tab.border": "#521081",
    "tab.inactiveBackground": "#1A0038",
    "tab.inactiveForeground": "#B1A2CA",
    "tab.hoverBackground": "#220044",
    "tab.unfocusedActiveForeground": "#D0C0E0",
    "panel.background": "#180033",
    "panel.border": "#2A0048",
    "panelTitle.activeForeground": "#E6D6F2",
    "panelTitle.inactiveForeground": "#9A87B2",
    "panelTitle.activeBorder": "#FF8FD6",
    "peekViewEditor.background": "#240040",
    "peekViewEditor.matchHighlightBackground": "#FFB84D44",
    "peekViewResult.background": "#1A0038",
    "peekViewResult.selectionBackground": "#30004A",
    "peekViewTitle.background": "#180033",
    "list.focusBackground": "#30004A",
    "list.focusForeground": "#F0E9F8",
    "list.activeSelectionBackground": "#3A0058",
    "list.activeSelectionForeground": "#F0E9F8",
    "list.inactiveSelectionBackground": "#240048",
    "list.hoverBackground": "#1E003D",
    "list.highlightForeground": "#F272B8",
    "list.errorForeground": "#F36F86",
    "list.warningForeground": "#F0B56A",
    "gitDecoration.addedResourceForeground": "#8FECA3",
    "gitDecoration.modifiedResourceForeground": "#7CC0F0",
    "gitDecoration.deletedResourceForeground": "#F25985",
    "gitDecoration.untrackedResourceForeground": "#AEEFD0",
    "gitDecoration.ignoredResourceForeground": "#5F4C71",
    "gitDecoration.conflictingResourceForeground": "#F0B56A",
    "button.background": "#FF77AA",
    "button.foreground": "#1A0A28",
    "button.hoverBackground": "#FF96C7",
