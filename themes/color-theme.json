    "scrollbarSlider.activeBackground": "#FF7CD0AA",
    "notificationCenterHeader.background": "#24004C",
    "notifications.background": "#1D0033",
    "notifications.foreground": "#D9D1E6",
    "notificationsErrorIcon.foreground": "#FF7A85",
    "notificationsWarningIcon.foreground": "#FFB84D",
    "notificationsInfoIcon.foreground": "#66D9FF",
