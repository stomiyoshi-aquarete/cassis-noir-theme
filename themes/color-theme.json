  "tokenColors": [
    {
      "scope": "comment",
      "settings": {
        "foreground": "#837786"
      }
    },
    {
      // This setting includes color of operator like `=` and `+`.
      "scope": "keyword",
      "settings": {
        "foreground": "#e182d6"
      }
    },
    {
      "scope": "storage.type",
      "settings": {
        "foreground": "#ee986e"
      }
    },
    {
      "scope": "keyword.control",
      "settings": {
        "foreground": "#cc6694",
        "fontStyle": "bold"
      }
    },
    {
      "scope": "storage.modifier",
      "settings": {
        "foreground": "#e9cbe2"
      }
    },
    {
      "scope": "operator",
      "settings": {
        "foreground": "#beaac6"
      }
    },
    {
      "scope": "entity.name.function",
      "settings": {
        "foreground": "#d452e3"
      }
    },
    {
      "scope": "entity.name.class",
      "settings": {
        "foreground": "#F2A4D6"
      }
    },
    {
      "scope": "entity.name.type",
      "settings": {
        "foreground": "#e47272"
      }
    },
    {
      "scope": "entity.name.tag",
      "settings": {
        "foreground": "#a489dd"
      }
    },
    {
      "scope": "variable",
      "settings": {
        "foreground": "#c478fb"
      }
    },
    {
      "scope": [
        "variable.language",
        "variable.other.constant"
      ],
      "settings": {
        "foreground": "#947aff"
      }
    },
    {
      "scope": "variable.parameter",
      "settings": {
        "foreground": "#bcb7d5"
      }
    },
    {
      "scope": "variable.other.property",
      "settings": {
        "foreground": "#d7bce4"
      }
    },
    {
      "scope": "string",
      "settings": {
        "foreground": "#e3aba5"
      }
    },
    {
      "scope": "string.regexp",
      "settings": {
        "foreground": "#dfab73"
      }
    },
    {
      "scope": "constant.numeric",
      "settings": {
        "foreground": "#b8b27a"
      }
    },
    {
      "scope": "constant.language",
      "settings": {
        "foreground": "#afb056"
      }
    },
    {
      "scope": "support.class",
      "settings": {
        "foreground": "#87aad3"
      }
    },
    {
      "scope": "support.function",
      "settings": {
        "foreground": "#d3849b"
      }
    },
    {
      "scope": "support.constant",
      "settings": {
        "foreground": "#cfa463"
      }
    },
    {
      "scope": "punctuation.definition.tag",
      "settings": {
        "foreground": "#d295bf"
      }
    },
    {
      "scope": "punctuation.definition.string",
      "settings": {
        "foreground": "#e2a898"
      }
    },
    {
      "scope": "punctuation.section.embedded",
      "settings": {
        "foreground": "#cb89ad"
      }
    },
    {
      "scope": "meta.directive",
      "settings": {
        "foreground": "#8cbcde"
      }
    },
    {
      "scope": "meta.import",
      "settings": {
        "foreground": "#de6cb8"
      }
    },
    {
      "scope": "markup.heading",
      "settings": {
        "foreground": "#e176ff",
        "fontStyle": "bold"
      }
    },
    {
      "scope": "markup.bold",
      "settings": {
        "foreground": "#ff82be",
        "fontStyle": "bold"
      }
    },
    {
      "scope": "markup.italic",
      "settings": {
        "foreground": "#EDD5F4",
        "fontStyle": "italic"
      }
    },
    {
      "scope": "markup.inline.raw",
      "settings": {
        "foreground": "#bc91e4"
      }
    },
    {
      "scope": "markup.quote",
      "settings": {
        "foreground": "#c6aae2"
      }
    },
    {
      "scope": "markup.list",
      "settings": {
        "foreground": "#fcd7f6"
      }
    },
    {
      "scope": "markup.inserted",
      "settings": {
        "foreground": "#8CE79A"
      }
    },
    {
      "scope": "markup.deleted",
      "settings": {
        "foreground": "#F25985"
      }
    },
    {
      "scope": "markup.changed",
      "settings": {
        "foreground": "#F0BF74"
      }
    },
    {
      "scope": "meta.diff.header",
      "settings": {
        "foreground": "#7CC0F0"
      }
    },
    {
      "scope": "constant.character.escape",
      "settings": {
        "foreground": "#7DD6F0"
      }
    },
    {
      "scope": "invalid",
      "settings": {
        "foreground": "#FFFFFF",
        "background": "#F25985"
      }
    },
    {
      "scope": "invalid.deprecated",
      "settings": {
        "foreground": "#FFFFFF",
        "background": "#F0BF74"
      }
    },
    {
      "scope": "variable.language.special.self",
      "settings": {
        "foreground": "#91b8c2",
        "fontStyle": "italic"
      }
    },
  ]
}
