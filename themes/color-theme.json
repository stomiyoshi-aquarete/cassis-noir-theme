    "terminal.ansiYellow": "#FFAD55",
    "terminal.ansiBlue": "#70D4FF",
    "terminal.ansiMagenta": "#FF7CC7",
    "terminal.ansiCyan": "#58D9D9",
    "terminal.ansiWhite": "#E0E0E0",
    "terminal.ansiBrightBlack": "#48395B",
    "terminal.ansiBrightRed": "#FF7797",
    "terminal.ansiBrightGreen": "#A7F6A7",
    "terminal.ansiBrightYellow": "#FFC773",
    "terminal.ansiBrightBlue": "#8ADFFF",
    "terminal.ansiBrightMagenta": "#FFA1DC",
    "terminal.ansiBrightCyan": "#77E6E6",
    "terminal.ansiBrightWhite": "#FFFFFF"
