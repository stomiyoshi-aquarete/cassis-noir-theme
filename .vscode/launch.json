{
  "$schema": "https://raw.githubusercontent.com/microsoft/vscode/main/extensions/vscode-api-tests/src/schemas/launch.schema.json",
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Run Extension",
      "type": "extensionHost",
      "request": "launch",
      "args": [
        "--extensionDevelopmentPath=${workspaceFolder}"
      ],
      "outFiles": [
        "${workspaceFolder}/dist/**/*.js"
      ]
    }
  ]
}
